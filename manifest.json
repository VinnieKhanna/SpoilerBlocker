{
    "manifest_version": 2,

    "name": "Undecided DelhiHacks extension",
    "description": "This extension will do something beautiful",
    "version": "1.0",

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "all_frames": true,
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}