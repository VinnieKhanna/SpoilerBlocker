{
    "manifest_version": 2,

    "name": "Heads-Up: Spoiler Blocker for Chrome",
    "description": "Surf the web without worrying about the next spoiler.",
    "version": "1.0",
    "icons": {
      "128": "newIcon.png"
    },

    "browser_action": {
     "default_icon": "newIcon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab"
     ]
  }